<div class="container">
  <h1>Welcome to your secret page!</h1>
    
        
    <b>First Name:</b> <%= @user.first_name %> </br></br>
    <b>Last Name:</b> <%= @user.last_name %> </br></br>
    <b>Email:</b> <%= @user.email %> </br></br>

    <table>
      <thead>
        <tr>
          <th>Regular Url</th>
          <th>Short Url</th>
          <th>Click Count</th>
        </tr>
      </thead>
      <tbody>
        <% @urls.each do |url| %>
        <tr>
          <td><%= url.url %></td>
          <td>/<%= url.short_url %></td>
          <td><%= url.click_counter %></td>
        </tr>
        <% end %>
      </tbody>
    </table></br></br>

    <a href="/logout">Log Out </a>

</div>
